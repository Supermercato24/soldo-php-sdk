{
  "name": "supermercato24/soldo-php-sdk",
  "description": "Unofficial PHP SDK to work with Soldo API",
  "keywords": ["supermercato24", "soldo", "api"],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Supermercato dev team",
      "homepage": "https://supermercato24.it"
    }
  ],
  "require": {
    "php": ">= 5.5",
    "guzzlehttp/guzzle": "~6.0",
    "psr/log": "^1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "symfony/var-dumper": "~3.3",
    "friendsofphp/php-cs-fixer": "~2.2"
  },
  "autoload": {
    "psr-4": {
      "Soldo\\": "src/Soldo/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Soldo\\Tests\\": "test/"
    }
  }
}
